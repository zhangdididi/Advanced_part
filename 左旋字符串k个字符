#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void left_move(char *str, int k, int len)
{
	int i = 0;
	int j = 0;
	char character = ' ';
	for (i = 0; i < k; i++)
	{
			character = str[0];
			for (j = 0; j < len - 1; j++)
			{
				str[j] = str[j + 1];
			}
			str[len - 1] = character;
	}
}
int main()
{
	int n = 0;
	int k = 1;
	scanf("%d", &k);
	char Letter[] = "ABCD";
	int len = strlen(Letter);
	left_move(Letter, k, len);
	for (n = 0; n < len; n++)
	{
		printf("%c ", Letter[n]);
	}
	printf("%d ", len);
	system("pause");
	return 0;
}
