#include <stdio.h>
#include <stdlib.h>
void Find(int arr[3][3], int *px, int *py, int key)
{
	int x = 0;
	int y = *py - 1;
	while(x < *px && y >= 0)
	 {
		if (key == arr[x][y])
		{
			*px = x;
			*py = y;
			return;
		}
		if(key < arr[x][y])
		{
			y--;
		}
		if (key > arr[x][y])
		{
			x++;
		}
	 }
	*px = -1;
	*py = -1;
}
int main()
{
	int  arr[3][3]= { 1, 2, 3,
                             2, 3, 4,
							 3, 4, 5};
	int x = 3;
	int y = 3;
	int key = 0;
	scanf("%d", &key);
	Find(arr, &x, &y, key);
	printf("%d %d\n", x, y);
	system("pause");
	return 0;
}
