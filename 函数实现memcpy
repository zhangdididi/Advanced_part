#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <assert.h>
#include <string.h>
void *Mymemcpy(void *dest, void *arr2, int count)
{
        assert(dest != NULL && arr2 != NULL);//断言
	void *p = dest;
	while (count != 0)
	{
		*(char *)dest = *(char*)arr2;
		*((char *)dest)++;
		*((char*)arr2)++;
		count--;
	}
	return p;//返回p为void *类型
}
int main()
{
	int arr1[20] = { 0 };
	int arr2[] = { 1, 2, 3, 4, 5, 6, 7, 8, 9 };
	Mymemcpy(arr1, arr2, 16);
	system("pause");
	return 0;
}
