#include <stdio.h>
int Mystrncmp(const char *str1, const char *str2, int n)
{
	int diff = 0;
	int count = 0;
	assert(str1 != NULL && str2 != NULL);
	while (count < n && ((diff = (*str1 - *str2))) == 0 && *str1 != '\0')
	{
		*str1++;
		*str2++;
		count++;
	}
	return diff;
}
int main()
{
	char *str1 = "abcdef";
	char *str2 = "abbde";
	int n = Mystrncmp(str1, str2, 5);
	printf("%d\n", n);
	system("pause");
	return 0;
}
