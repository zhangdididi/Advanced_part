#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <stdlib.h>

void left_move(char *str, int k, int sz)
{
	int i = 0;
	int j = 0;
	char character = ' ';
	for (i = 0; i < k; i++)
	{
			character = str[0];
			for (j = 0; j < sz - 1; j++)
			{
				str[j] = str[j + 1];
			}
			str[sz - i - 1] = character;
	}
}
int main()
{
	int n = 0;
	int k = 1;
	scanf("%d", &k);
	char Letter[] = { 'A','B','C','D' };
	int sz = sizeof(Letter) / sizeof(Letter[0]);
	left_move(Letter, k, sz);
	for (n = 0; n < sz; n++)
	{
		printf("%c ", Letter[n]);
	}
	system("pause");
	return 0;
}
