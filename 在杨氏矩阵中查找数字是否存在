#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <stdlib.h>

int Search_num(int arr[3][3], int ret)
{
	int row = 0;
	int col = 2;
	while ((row <= 2) && (col >= 0))
	{
		if (arr[row][col] > ret)
			col--;
		else if (arr[row][col] < ret)
			row++;
		else
			return 1;
	}
	return -1;
}

int main()
{
	int n = 0;
	int arr[3][3] = { 1,2,3,2,3,4,3,4,5 };
	int num = 0;
	scanf("%d", &num);
	n = Search_num(arr, num);
	if (1 == n)
	{
		printf("%d is in arr\n", num);
	}
	if (-1 == n)
	{
		printf("%d is not in arr\n", num);
	}
	system("pause");
	return 0;
}
