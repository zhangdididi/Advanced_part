#include <stdio.h>
//自己实现整形比较
int Int_cmp(const void *elem1, const void *elem2)
{
	return *((int *)elem1) - *(int *)elem2);
}
int main()
{
	int arr[10] = { 2, 3, 1, 54, 233, 15, 32, 82, 12, 10 };//初始化数组
	qsort(arr, sizeof(arr)/ sizeof(arr[0]), sizeof(int), Int_cmp);
	for (int i = 0; i < 10; i++)
	{
		printf("%d ", arr[i]);//打印数组
	}
	return 0;
}
